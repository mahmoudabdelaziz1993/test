<form method="POST" action="/test">
  <table class="table mw-100 flex-grow-1 ">
    <thead class="thead-dark">
      <tr>
        <th scope="col">num</th>
        <%keys.map(function (key) {%>
        <th scope="col"><%= key %></th>
        <% }); %>
        <th scope="col">submit</th>
      </tr>

    </thead>
    <tbody>
      <%tickets.map(function (ticket , index) {%>
      <tr>
        <th scope="row"><%= index+1 %></th>
        <%let ticketkeys = Object.keys(ticket)%>
        <%ticketkeys.map(function (key) {%>
        <td><%= ticket[key] %></td>
        <% }); %>
        <td>
          <input type="checkbox" class="form-check-input" id="exampleCheck1" name="documents" value=<%=index%> >
        </td>
      </tr>
      <% }); %>



  </table>
  <button type="submit" class="btn btn-success"><i class="fa fa-check"></i>Submit </button>
</form>